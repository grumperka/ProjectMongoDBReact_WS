{
  "spring": {
    "application": {
      "name": "eureka-WS-service"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false
    },
    "instance": {
      "hostname": "localhost",
      "port": 5000
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CoreDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "MongoDBDatabaseSettings": {
    "PokojCollectionName": "pokoje",
    "RezerwacjaCollectionName": "rezerwacje",
    "RachunekCollectionName": "rachunki",
    "KlientCollectionName": "klienci",
    "PracownikCollectionName": "pracownicy",
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "bazaDanych"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "ProjectMongoDBReact": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*"
}
